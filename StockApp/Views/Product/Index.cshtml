@model IEnumerable<StockApp.Models.Product>

<h1>Produtos</h1>
<table>
    <thead>
        <tr>
            <th>Código</th>
            <th>Descrição</th>
            <th>Quantidade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductId</td>
                <td>@product.Description</td> 
                <td>@product.Quantity</td>
            </tr>
        }
    </tbody>
</table>